{
    "openapi": "3.0.0",
    "info": {
        "title": "Open Commerce/Qubit API",
        "version": "1.0.0-beta"
    },
    "servers": [
        {
            "url": "/api/v1",
            "description": "Development server"
        }
    ],
    "paths": {
        "/shop": {
            "get": {
                "operationId": "primaryShop",
                "parameters": {},
                "responses": {
                    "200": {
                        "description": "Gets the primary shop",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Shop"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/orders/{shopId}": {
            "get": {
                "operationId": "orders",
                "parameters": [
                    {
                        "in": "path",
                        "name": "shopId",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Get a list of orders by shop",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Orders"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/cart": {
            "post": {
                "operationId": "createDraftOrderCart",
                "summary": "Creates a new cart",
                "parameters": [
                    {
                        "in": "body",
                        "name": "input",
                        "description": "The cart to create",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "input": {
                                    "type": "object",
                                    "properties": {
                                        "createCartInput": {
                                            "type": "object",
                                            "properties": {
                                                "items": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "object",
                                                        "properties": {
                                                            "price": {
                                                                "type": "object",
                                                                "properties": {
                                                                    "amount": {
                                                                        "type": "number"
                                                                    },
                                                                    "currencyCode": {
                                                                        "type": "string"
                                                                    }
                                                                }
                                                            },
                                                            "productConfiguration": {
                                                                "type": "object",
                                                                "properties": {
                                                                    "productId": {
                                                                        "type": "string"
                                                                    },
                                                                    "productVariantId": {
                                                                        "type": "string"
                                                                    }
                                                                }
                                                            },
                                                            "quantity": {
                                                                "type": "integer"
                                                            }
                                                        }
                                                    }
                                                }
                                            }
                                        },
                                        "shopId": {
                                            "type": "string"
                                        },
                                        "draftOrderId": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Creates a new cart",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/DraftOrder"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
