                    },
                    "currency": {
                        "type": "object"
                    },
                    "emailRecords": {
                        "type": "array",
                        "items": {
                            "type": "object"
                        }
                    },
                    "firstName": {
                        "type": "string"
                    },
                    "language": {
                        "type": "string"
                    },
                    "phone": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "metafields": {
                        "type": "array",
