{
    "endpoints": {
        "/shop": {
            "get": {
                "operation": "primaryShop"
            }
        },
        "/orders/:shopId": {
            "get": {
                "operation": "orders",
                "params": {
                    "shopId": "shopId"
                }
            }
        },
        "/orders": {
            "post": {
                "operation": "placeOrderFromDraftOrder",
                "successStatusCode": 201
            }
        },
        "/catalog-items/:shopId": {
            "get": {
                "operation": "catalogItems",
                "params": {
                    "shopId": "shopId"
                }
            }
        },
        "cart/:shopId/:accountId": {
            "get": {
                "operation": "accountCartByAccountId",
                "params": {
                    "shopId": "shopId",
                    "accountId": "accountId"
                }
            }
        },
        "cart": {
            "post": {
                "operation": "createDraftOrderCart",
                "successStatusCode": 201
            }
        },
        "cart/:cartId/cart-items/add": {
            "post": {
                "operation": "addDraftOrdersCartItems"
            }
        },
        "cart/:cartId/cart-items/update": {
            "post": {
                "operation": "updateCartItemsQuantityFromDraftOrders"
            }
        }
    },
    "errors": {
        "errorCodes": {
            "UNAUTHENTICATED": {
                "httpCode": 401,
                "errorDescription": "Forbidden: Unauthorized access"
            },
            "X-InternalServerError": {
                "httpCode": 500,
                "errorDescription": "Empty object: body parser"
            }
        }
    }
}