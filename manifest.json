{
    "endpoints": {
        "/shop": {
            "get": {
                "operation": "primaryShop"
            }
        },
        "/orders/:shopId": {
            "get": {
                "operation":"orders",
                "params": {
                    "shopId": "shopId"
                }
            },
            "post": {
                "operation": "placeOrder"
            }
        },
        "/catalog-items/:shopId": {
            "get": {
                "operation": "catalogItems",
                "params": {
                    "shopId": "shopId"
                }
            }
        }
    },
    "errors": {
        "errorCodes": {
            "UNAUTHENTICATED": {
                "httpCode": 401,
                "errorDescription": "Forbidden: Unauthorized access"
            },
            "X-InternalServerError": {
                "httpCode": 500,
                "errorDescription": "Empty object: body parser"
            }
        }
    }
}